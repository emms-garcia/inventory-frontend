doctype html
html.no-js
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		title Inventory Web
		meta(name='description', content='')
		meta(name='author', content='')
		// Place favicon.ico and apple-touch-icon.png in the root directory
		// build:css(.) styles/vendor.css
		// bower:css
		link(rel='stylesheet', href='bower_components/bootstrap/dist/css/bootstrap.css')
		link(rel='stylesheet', href='bower_components/angular-xeditable/dist/css/xeditable.css')
		link(rel='stylesheet', href='bower_components/angular-ui-notification/dist/angular-ui-notification.min.css')
		// endbower
		// endbuild
		// build:css(.tmp) styles/main.css
		link(rel='stylesheet', href='styles/main.css')
		link(rel='stylesheet', href='styles/overwrites.css')
		// endbuild
	body(ng-app='inventoryApp')
		//if lt IE 7
			p.browsehappy
				| You are using an 
				strong outdated
				|  browser. Please 
				a(href='http://browsehappy.com/') upgrade your browser
				|  to improve your experience.
		// Add your site or application content here
		div(ui-view="headerContainer")
		.container-fluid
			.row
				div(ui-view="sidebarContainer")
				div(ui-view="mainContainer")
					div(ui-view="mainView")
		// Google Analytics: change UA-XXXXX-X to be your site's ID
		script.
			!function(A,n,g,u,l,a,r){A.GoogleAnalyticsObject=l,A[l]=A[l]||function(){
			(A[l].q=A[l].q||[]).push(arguments)},A[l].l=+new Date,a=n.createElement(g),
			r=n.getElementsByTagName(g)[0],a.src=u,r.parentNode.insertBefore(a,r)
			}(window,document,'script','//www.google-analytics.com/analytics.js','ga');
			ga('create', 'UA-XXXXX-X');
			ga('send', 'pageview');
		// build:js(.) scripts/vendor.js
		// bower:js
		script(src='bower_components/jquery/dist/jquery.js')
		script(src='bower_components/angular/angular.js')
		script(src='bower_components/angular-animate/angular-animate.js')
		script(src='bower_components/angular-resource/angular-resource.js')
		script(src='bower_components/angular-route/angular-route.js')
		script(src='bower_components/angular-local-storage/dist/angular-local-storage.js')
		script(src='bower_components/ui-router/release/angular-ui-router.js')
		script(src='bower_components/angular-translate/angular-translate.js')
		script(src='bower_components/angular-bootstrap/ui-bootstrap.js')
		script(src='bower_components/angular-bootstrap/ui-bootstrap-tpls.js')
		script(src='bower_components/angular-xeditable/dist/js/xeditable.js')
		script(src='bower_components/angular-ui-notification/dist/angular-ui-notification.min.js')
		// endbower
		// endbuild
		// build:js({.tmp,app}) scripts/scripts.js
		script(src='scripts/app.js')
		script(src="scripts/constants.js")

		// Controllers
		script(src='scripts/controllers/header-controller.js')
		script(src='scripts/controllers/sidebar-controller.js')
		script(src='scripts/controllers/login-controller.js')
		script(src='scripts/controllers/dashboard-controller.js')
		script(src='scripts/controllers/profile-controller.js')
		script(src='scripts/controllers/edit-password-controller.js')

		// Services
		script(src='scripts/services/translation-loader-service.js')
		script(src="scripts/services/user-service.js")
		script(src="scripts/services/utils-service.js")

		// endbuild
